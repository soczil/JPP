int main() {
    int i = 0;
    while (i <= 3) {
        if (i == 0) {
            print("zero");
        }
        elif (i == 1) {
            print("jeden");
        }
        elif (i == 2) {
            print("dwa");
        }
        else {
            print("trzy");
        }
        i++;
    }

    return 0;
}
